    <!-- Vendor Scripts Standalone Libraries -->
    <!-- <script src="js/vendor/all.js"></script> -->
    <script src="js/vendor/jquery.js"></script>
    <script src="js/vendor/bootstrap.js"></script>
    <script src="js/vendor/breakpoints.js"></script>
    <script src="js/vendor/jquery.nicescroll.js"></script>
    <script src="js/vendor/isotope.pkgd.js"></script>
    <script src="js/vendor/packery-mode.pkgd.js"></script>
    <script src="js/vendor/jquery.grid-a-licious.js"></script>
    <script src="js/vendor/jquery.cookie.js"></script>
    <script src="js/vendor/jquery-ui.custom.js"></script>
    <script src="js/vendor/jquery.hotkeys.js"></script>
    <script src="js/vendor/handlebars.js"></script>
    <script src="js/vendor/jquery.hotkeys.js"></script>
    <script src="js/vendor/load_image.js"></script>
    <script src="js/vendor/jquery.debouncedresize.js"></script>
    <script src="js/vendor/forms/all.js"></script>
    <script src="js/vendor/media/all.js"></script>
    <!-- <script src="js/vendor/angular/all.js"></script> -->

    <!-- App Scripts Bundle
    Includes Custom Application JavaScript used for the current theme/module;
